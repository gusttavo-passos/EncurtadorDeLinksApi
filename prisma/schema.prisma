generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Links {
  id        String   @id @default(uuid())
  userId    String
  url       String
  shortUrl  String
  createdAt DateTime @default(now())
}
